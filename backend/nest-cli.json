{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": ["../data/*.data.json"],
    "watchAssets": true
  },
  "watchOptions": {
    "exclude": ["src/properties/properties.data.ts"]
  }
}